{
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "css.customData": [".vscode/css_custom_data.json"],
  "tailwindCSS.includeLanguages": {
    "css": "css",
    "scss": "scss"
  },
  "tailwindCSS.experimental.classRegex": [
    ["className\\s*=\\s*[\"']([^\"']*)[\"']", "([^\\s\"']+)"],
    ["class\\s*=\\s*[\"']([^\"']*)[\"']", "([^\\s\"']+)"]
  ],
  "files.associations": {
    "*.css": "tailwindcss"
  }
}